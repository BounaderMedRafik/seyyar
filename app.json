{
  "expo": {
    "name": "Seyyar",
    "slug": "seyyar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-no-rounds.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seyyar.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-no-rounds.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.seyyar.app",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-no-rounds.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0fb7674a-b7f5-442f-87e2-05bf32a305f0"
      }
    }
  }
}
